{
 "apiVersion": "apps/v1",
 "kind": "Deployment",
 "metadata": {
  "name": "REPOSITORY.IMAGE.DOCKER",
  "labels": {
   "app": "REPOSITORY.IMAGE.DOCKER"
  }
 },
 "spec": {
  "template": {
   "spec": {
    "containers": [
     {
      "name": "REPOSITORY.IMAGE.DOCKER",
      "env": [
       {
        "name": "API_NAME",
        "value": "hello.app.flask"
       }
      ]
     }
    ]
   }
  }
 }
}