{
 "apiVersion": "networking.k8s.io/v1beta1",
 "kind": "Ingress",
 "metadata": {
  "name": "REPOSITORY.IMAGE.DOCKER",
  "annotations": {
   "nginx.ingress.kubernetes.io/rewrite-target": "/"
  }
 },
 "spec": {
  "rules": [
   {
    "http": {
     "paths": [
      {
       "path": "/",
       "backend": {
        "serviceName": "REPOSITORY.IMAGE.DOCKER",
        "servicePort": 80
       }
      }
     ]
    }
   }
  ]
 }
}